contract Charityable is Context, Ownable {
    address payable private _charity;

    function charity() public view returns (address payable) {
        return _charity;
    }

    modifier onlyCharity() {
        require(_charity == _msgSender(), "Caller is not the charity address");
        _;
    }

    function setCharityAddress(address payable charityAddress)
        public
        virtual
        onlyOwner
    {
        require(
            _charity == address(0),
            "Charity address cannot be changed once set"
        );
        _charity = charityAddress;
    }
}

